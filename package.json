    "prettier": "^3.5.0",
    "turbo": "^2.4.2",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@11.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@octokit/rest": "^21.1.1",
    "@types/node": "^22.13.5",
    "chalk": "^4.1.2",
    "framer-motion": "^12.4.7",
    "fs": "^0.0.1-security",
    "simple-git": "^3.27.0"
  }
